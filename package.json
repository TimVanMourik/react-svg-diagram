{
  "name": "react-svg-diagram",
  "version": "0.0.0",
  "description": "A react component for building an diagram as svg elements",
  "main": "index.js",
  "author": "TimVanMourik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TimVanMourik/react-svg-diagram/issues"
  },
  "homepage": "https://github.com/TimVanMourik/react-svg-diagram#readme",
  "scripts": {
    "start": "run-p storybook:start",
    "build": "run-p clean library:build:commonjs library:build:es library:build:umd library:build:umd_min storybook:build",
    "storybook:start": "start-storybook -p 9001 -s storybook/public -c storybook",
    "storybook:build": "build-storybook -s storybook/public -c storybook -o build-storybook",
    "storybook:publish": "gh-pages -m 'Update gh-pages' -d ./build-storybook",
    "library:build:commonjs": "cross-env KEEP_MODULES=false babel --out-dir build-commonjs src",
    "library:build:es": "cross-env KEEP_MODULES=true babel --out-dir build-es src",
    "library:build:umd_min": "cross-env webpack --env.minimize",
    "library:build:umd": "webpack",
    "clean": "del build-*",
    "version": "yarn build",
    "postpublish": "yarn storybook:publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimVanMourik/react-svg-diagram.git"
  },
  "keywords": [
    "react",
    "svg",
    "diagram",
    "diagram",
    "diagrams",
    "flowchart",
    "nodes",
    "edges",
    "ports",
    "links"
  ],
  "dependencies": {
    "pretty": "^2.0.0",
    "prop-types": "^15.7.2",
    "react-redux": "^7.1.1",
    "react-svg-pan-zoom": "^3.5.0",
    "redux": "^4.0.4",
    "redux-orm": "^0.15.1",
    "save-file": "^2.3.1",
    "save-svg-as-png": "^1.4.14",
    "transformation-matrix": "^2.1.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@storybook/addon-actions": "5.2.6",
    "@storybook/addon-knobs": "5.2.6",
    "@storybook/addon-links": "5.2.6",
    "@storybook/addons": "5.2.6",
    "@storybook/react": "5.2.6",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/theming": "^5.1.10",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "del-cli": "^2.0.0",
    "gh-pages": "^2.1.1",
    "lorem-ipsum": "^2.0.3",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "^3.2.0",
    "react-virtualized": "^9.21.1",
    "remarkable": "^2.0.0",
    "sass-loader": "^8.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  }
}
